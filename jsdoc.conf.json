{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown","jsdoc-plugin-typescript"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "Clock",
    "footer": "",
    "copyright": "js-clock Copyright Â© 2015, 2017, 2023 Daniel Potter.",
    "navType": "vertical",
    "theme": "cosmo",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "highlightTutorialCode": true,
    "protocol": "html://"
  },
  "markdown": {
    "parser": "gfm"
  },
  "typescript": {
    "moduleRoot": "src"
  }
}
